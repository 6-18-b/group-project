{
  "name": "mused",
  "version": "0.0.1",
  "description": "arts app",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node build/scripts/compile",
    "build": "npm run clean && npm rebuild node-sass  &&  npm run build-css  && cross-env NODE_ENV=production npm run compile",
    "build:prod": "echo \"build:prod is deprecated. Use npm run build.\" && npm run build",
    "build:functions": "npm run build --prefix functions",
    "deploy": "firebase-ci deploy -s",
    "createConfig": "firebase-ci createConfig",
    "start": "cross-env NODE_ENV=development webpack-dashboard -p 1234 -c cyan -m -t mused -- node build/scripts/start",
    "start:simple": "cross-env NODE_ENV=development node build/scripts/start",
    "test": "cross-env NODE_ENV=test karma start build/karma.config",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src functions server build project.config.js tests",
    "lint:fix": "npm run lint -- --fix",
    "functions:start": "npm run start --prefix functions",
    "functions:build": "npm run build --prefix functions",
    "functions:test": "npm run test --prefix functions",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "create-config": "node build/create-config",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SabrinaDanielle/mused.git"
  },
  "engines": {
    "node": ">=6.14.0"
  },
